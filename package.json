    "ky": "^0.24.0",
    "lodash": "^4.17.20",
    "loglevel": "^1.7.0",
    "loglevel-plugin-remote": "^0.6.8",
    "react": "^16.14.0",
    "react-app-polyfill": "^1.0.6",
    "react-custom-scrollbars": "^4.2.1",
    "react-dev-utils": "^10.2.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "redux-act": "^1.8.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "resolve": "^1.17.0",
