{
  "name": "colonq",
  "version": "0.9.0-alpha",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "cosmos": "cosmos --config cosmos/cosmos.config.json",
    "cosmos:export": "cosmos-export --config cosmos/cosmos.config.json"
  },
  "browserslist": {
    "production": [
      ">0.1% in RU",
      "not dead",
      "not op_mini all",
      "not IE > 0"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "dependencies": {
    "autoprefixer": "^9.7.4",
    "babel-eslint": "10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.1",
    "bootstrap-utilities": "^4.1.3",
    "camelcase": "^5.3.1",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "clsx": "^1.1.0",
    "colorguard": "^1.2.1",
    "connected-react-router": "^6.8.0",
    "css-loader": "3.4.2",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "event-source-polyfill": "^1.0.12",
    "file-loader": "4.3.0",
    "flexsearch": "github:nextapps-de/flexsearch#0.7.0",
    "fs-extra": "^8.1.0",
    "fuse.js": "^5.2.3",
    "history": "^4.10.1",
    "html-webpack-plugin": "4.0.0-beta.11",
    "idb": "^5.0.3",
    "identity-obj-proxy": "3.0.0",
    "immutability-helper": "^3.0.2",
    "is-wsl": "^1.1.0",
    "ky": "^0.19.0",
    "lodash": "^4.17.19",
    "loglevel": "^1.6.8",
    "loglevel-plugin-remote": "^0.6.8",
    "react": "^16.13.0",
    "react-app-polyfill": "^1.0.6",
    "react-custom-scrollbars": "^4.2.1",
    "react-dev-utils": "^10.2.0",
    "react-dom": "^16.13.0",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "resolve": "1.15.0",
    "resolve-url-loader": "3.1.1",
    "sass": "^1.26.3",
    "sass-loader": "8.0.2",
    "semver": "6.3.0",
